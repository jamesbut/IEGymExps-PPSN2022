{
    "optimiser": {
        "cmaes": {
            "centroid": [0.0],
            "sigma": 1.0,
            "lambda": 100,
            "fix_C": true,
            "fix_sigma": true,
            "fix_centroid": true,
            "verbosity": 0
        },
        "num_gens": 20,
        "parallelise": true,
        "render": false,
        "quit_domain_when_complete": false,
        "p_lb": [-100.0],
        "p_ub": [100.0]
    },
    "controller": {
        "num_hidden_layers": 1,
        "neurons_per_hidden_layer": 4,
        "bias": true,
        "hidden_layer_activ_func": "relu",
        "final_layer_activ_func": "sigmoid"
    },
    "env": {
        "name": "FrozenLake-v0",
        "env_kwargs": {
            "is_slippery": false,
            "reward_fnc": "manhattan",
            "goal_pos": {
                "x": 3,
                "y": 3
            }
        },
        "completion_fitness": 0.0,
        "domain_params": [{"x": 2, "y": 1}],
        "normalise_state": false,
        "seed": 108,
        "verbosity": 0
    },
    "ie": {
        "name": "ae",
        "use_decoder": false,
        "decoder_file_num": 10,
        "code_size": 1,
        "num_hidden_layers": 1,
        "neurons_per_hidden_layer": 64,
        "num_epochs": 10000,
        "batch_size": 256,
        "optimiser": {
            "name": "adam",
            "lr": 1e-3
        },
        "training_data_dir": "cmc_configs/cmc_train_data",
        "exp_group": false,
        "dump_model_dir": "models/cmc_models",
        "num_trains": 5
    },
    "execution": {
        "num_runs": 1
    },
    "logging": {
        "data_dir_path": "data/",
        "winner_file_name": "best_winner_so_far",
        "save_if_wb_exceeded": true,
        "save_winners_only": false
    }
